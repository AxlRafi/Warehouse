            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/container_frame1"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingBottom="16dp"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:paddingTop="16dp">

            <ImageView
                android:id="@+id/image_frame1"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_frame1" />

            <LinearLayout
                android:id="@+id/container_frame3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-0.5dp"
                android:layout_marginBottom="-0.5dp"
                android:clipToPadding="false"
                android:gravity="left|top"
                android:orientation="vertical"
                android:paddingBottom="0.5dp"
                android:paddingTop="0.5dp">

                <TextView
                    android:id="@+id/text_account_balance"
                    android:layout_width="match_parent"
                    android:layout_height="16dp"
                    android:layout_marginTop="-0.5dp"
                    android:layout_marginBottom="-0.5dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Account Balance"
                    android:textColor="#FF000000"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/text_balance_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginTop="-0.5dp"
                    android:layout_marginBottom="-0.5dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Rp 1.000.000"
                    android:textColor="#FF000000"
                    android:textSize="14dp" />
            </LinearLayout>
            <!-- 8:34:space-view1 Frame 3_space_view1 space-view -->
            <View
                android:layout_width="0dp"
                android:layout_height="32dp"
                android:layout_weight="1" />

            <ImageView
                android:id="@+id/image_frame2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_frame2" />

            <LinearLayout
                android:id="@+id/container_frame5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="-0.5dp"
                android:layout_marginBottom="-0.5dp"
                android:clipToPadding="false"
                android:gravity="left|top"
                android:orientation="vertical"
                android:paddingBottom="0.5dp"
                android:paddingTop="0.5dp">

                <TextView
                    android:id="@+id/text_bill_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="-0.5dp"
                    android:layout_marginBottom="-0.5dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Bill Amount"
                    android:textColor="#FF000000"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/text_bill_amount_value"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:layout_marginTop="-0.5dp"
                    android:layout_marginBottom="-0.5dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Rp 125.000"
                    android:textColor="#FF000000"
                    android:textSize="14dp" />
            </LinearLayout>
            <!-- 8:46:space-view1 Frame 5_space_view1 space-view -->
            <View
                android:layout_width="0dp"
                android:layout_height="32dp"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

